{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "client/dist",
    "installCommand": "npm install",
    "framework": null,
    "devCommand": "npm run dev",
    "functions": {
        "api/**/*.ts": {
            "runtime": "@vercel/node@3"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}